                <div class="box-50 box-mob-100">
                    <div class="formcontato" id="hr">
                        <h2>CADASTRO</h2>
                        <div class="linha second"></div>
                        <form action="/register" method="post">
                            <div class="form-container-input">
                                <label for="name">Nome</label>
                                <input class="form-input" placeholder="Seu Nome" type="name" required id="name_cadastro"
                                    name="name">
                            </div>
                            <div class="form-container-input">
                                <label for="email">E-mail</label>
                                <input class="form-input" placeholder="exemplo@gmail.com" type="email" required id="email_cadastro"
                                    name="email">
                            </div>
                            <div class="form-container-input">
                                <label for="telefone">Telefone</label>
                                <input class="form-input" placeholder="(99) 99999-9999" type="text" required id="tel-cadastro"
                                    name="telefone">
                            </div>
                            <div class="form-container-input">
                                <label for="password">Senha</label>
                                <input class="form-input" placeholder="*********" type="password" required id="senha_cadastro"
                                    name="password">
                            </div>
                            <!-- <div class="form-container-input">
                                <label for="email">Confirme senha</label>
