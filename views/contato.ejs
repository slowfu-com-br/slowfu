    <div class="container">
      <div class="formcontato">
        <form action="/formulario-contato" method="post">
          <div>
            <label for="nome">Nome</label>
            <input placeholder="Nome e Sobrenome" type="texto" id="nome" name="nome_usuario">
          </div>
          <div>
            <label for="email">E-mail</label>
            <input placeholder="exemplo@gmail.com" type="email" id="email" name="email_usuario">
          </div>
          <div>
            <label for="telefone">Telefone</label>
            <input placeholder="(99) 99999-9999" type="telefone" id="telefone" name="telefone_usuario">
          </div>
          <div class="input-texto-contato">
            <label for="texto">Texto</label>
            <textarea rows="4" placeholder="Digite um texto aqui..." type="texto" id="texto" name="texto_usuario"></textarea height="150">
          </div>
          <div class="button_contato">
            <button type="submit">Enviar</button>
          </div>
      </form>
    </div>
